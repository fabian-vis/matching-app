<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body>

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>   
        <section>
            <h2>Welcome <%= profiel.voornaam %> <%= profiel.achternaam %></h2>
            <h1>Results</h1>
            <h3>Filter</h3>
        </section>   

        <form method='POST' action="/liked">
            <section>
                    <div class="profielstyling">

                            <img src="../img/fabian.jpeg" class="profielfoto" alt="Profielfoto van fabian vis"/>
                                <div class="naamSysteem">
                                    <h1>Fabian Vis</h1>
                                    <p>Shooters/MMO</p>
                                    <img src="../img/xbox.png" alt="foto van xboxlogo"/>
                                </div>
                                <div class="checkbox">
                                    <label class="container">
                                        <input name="personen" type="checkbox" value="Fabian Vis">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                    </div>
            </section> 

            <section>
                <div class="profielstyling">

                        <img src="../img/deaap2.jpg" class="profielfoto" alt="Profielfoto van fabian vis"/>
                            <div class="naamSysteem">
                                <h1>deaap2</h1>
                                <p>Shooters/MMO</p>
                                <img src="../img/xbox.png" alt="foto van xboxlogo"/>
                            </div>
                            <div class="checkbox">
                                <label class="container">
                                    <input name="personen" type="checkbox" value="deaap2">
                                    <span class="checkmark"></span>
                                  </label>
                            </div>
                </div>
        </section> 
            <button type="submit" id="submitbutton">Submit</button>
        </form>

    </main>
    <script src="https://use.fontawesome.com/0b90345e46.js"></script>
    <footer>
        <%- include('../partials/footer'); %>
    </footer>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
    <script src="../scripts/script.js"></script>
</body>
</html>