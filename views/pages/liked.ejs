<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head2');%>
</head>
<body>

    <header>
        <%- include('../partials/header');%>
    </header>

    <main>   
        <section>
            <h2>Welcome <%= profiel.voornaam %> <%= profiel.achternaam %></h2>
            <h1>People you Liked</h1>
            <h3>Filter</h3>
        </section> 

        <section>
            <ul>
                <% personen.forEach(function(persoon) { %>
                <li>
                    <h1><%= persoon.persoon %></h1>
                    <button data-id="<%=persoon._id%>">Remove</button>
                </li>
                <% });%>
            </ul>
        </section>  
    </main>
    <script src="https://use.fontawesome.com/0b90345e46.js"></script>
    <footer>
        <%- include('../partials/footer');%>
    </footer>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" type="text/javascript"></script>
    <script src="../scripts/script.js"></script>
</body>
</html>